# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[{.eslintrc.json, eslintrc.*.json, project.json, nx.json, tsconfig.json, tsconfig.*.json, package.json, package-lock.json}]
indent_size = 2
indent_size = space